{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "atables/table",
	"version": "1.0.0",
	"title": "A-Tables Table",
	"category": "widgets",
	"icon": "grid-view",
	"description": "Display a table created with A-Tables & Charts plugin.",
	"keywords": ["table", "data", "spreadsheet", "atables"],
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"attributes": {
		"tableId": {
			"type": "number",
			"default": 0
		}
	},
	"textdomain": "a-tables-charts",
	"editorScript": "file:./index.js"
}
